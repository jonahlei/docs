{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Seed 文档",
  "colors": {
    "primary": "#0084ff",
    "light": "#66b5ff",
    "dark": "#004f99"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "zh",
        "tabs": [
          {
            "tab": "教程",
            "groups": [
              {
                "group": "开始使用",
                "pages": [
                  "docs_zh/get_started/quickstart",
                  "docs_zh/get_started/api_key",
                  "docs_zh/get_started/model_list"
                ]
              },
              {
                "group": "开发指南",
                "pages": [
                  "docs_zh/developer_guide/deep_thinking",
                  "docs_zh/developer_guide/visual_understanding",
                  "docs_zh/developer_guide/image_generation",
                  "docs_zh/developer_guide/video_generation",
                  "docs_zh/developer_guide/text_generation"
                ]
              },
              {
                "group": "最佳实践",
                "pages": [
                  "docs_zh/best_practices/image_prompt_guide",
                  "docs_zh/best_practices/video_prompt_guide"
                ]
              },
              {
                "group": "常见问题",
                "pages": ["docs_zh/faq"]
              },
              {
                "group": "法务协议",
                "pages": ["docs_zh/legal/legal_agreement"]
              }
            ]
          },
          {
            "tab": "API 参考",
            "groups": [
              {
                "group": "简介",
                "pages": [
                  "api_reference_zh/authentication",
                  "api_reference_zh/error-codes",
                  "api_reference_zh/rate_limiting"
                ]
              },
              {
                "group": "Chat API",
                "openapi": "api_json_zh/openapi.json",
                "pages": ["post /chat/completions"]
              },
              {
                "group": "Responses API",
                "openapi": "api_json_zh/openapi.json",
                "pages": ["post /responses"]
              },
              {
                "group": "图片生成",
                "openapi": "api_json_zh/openapi.json",
                "pages": ["post /images/generations"]
              },
              {
                "group": "视频生成",
                "openapi": "api_json_zh/openapi.json",
                "pages": [
                  "post /contents/generations/tasks",
                  "get /contents/generations/tasks/{id}",
                  "get /contents/generations/tasks",
                  "delete /contents/generations/tasks/{id}"
                ]
              },
              {
                "group": "向量化",
                "openapi": "api_json_zh/openapi.json",
                "pages": ["post /embeddings", "post /embeddings/multimodal"]
              },
              {
                "group": "openapi API",
                "openapi": "api_json_zh/openai_documented.json"
               
              }
            ]
          },
          {
            "tab": "发布说明",
            "groups": [
              {
                "group": " ",
                "pages": ["releasenotes_zh/releasenotes"]
              }
            ]
          }
        ]
      }
    ]
  },
  "api": {
    "playground": {
      "display": "interactive"
    },
    "mdx": {
      "server": "https://ark.cn-beijing.volces.com/api/v3",
      "auth": {
        "method": "bearer",
        "name": "API Key"
      }
    },
    "examples": {
      "languages": ["curl", "python", "javascript", "go", "java"]
    }
  },
  "logo": {
    "light": "/logo/logo_light.svg",
    "dark": "/logo/logo_dark.svg",
    "href": "https://console.volcengine.com/ark"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "体验中心",
      "href": "https://console.volcengine.com/ark/region:ark+cn-beijing/experience/?projectName=default"
    }
  },
  "footer": {
    "socials": { "github": "https://github.com/volcengine" }
  },
  "contextual": {
    "options": ["copy", "view", "mcp", "vscode"]
  }
}
